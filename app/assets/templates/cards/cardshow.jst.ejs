
<span class="card__body"><%= card.escape('title') %></li>
	



<i class="fa fa-trash-o pull-right" data-toggle="modal" 						data-target="#<%=modalID%>"></i>


<div class="modal fade" id="<%=modalID%>" role="dialog" 						aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" 					aria-hidden="true">&times;</button>
        <h4 class="modal-title" id="myModalLabel"><%= 					card.escape('title')%></h4>
      </div>
      <div class="modal-body">
        <p>Are you sure you want to delete this card?</p>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" 					data-dismiss="modal">Close</button>
        <button type="button" class="deleteCard btn btn-primary">Save 					changes</button>
      </div>
    </div>
  </div>
</div>